vocabulary <http://ontologia-tcc/vocabulary/base#> as base {

	extends <http://www.w3.org/2001/XMLSchema#> as xsd

	aspect EntidadeNomeavel
	aspect EntidadeDescritivel
	aspect EntidadeIdentificavel [
		key id
	]

	scalar property nome [
		domain EntidadeNomeavel
		range xsd:string
		functional
	]
	scalar property descricao [
		domain EntidadeDescritivel
		range xsd:string
		functional
	]
	scalar property id [
		domain EntidadeIdentificavel
		range xsd:string
		functional
	]
}